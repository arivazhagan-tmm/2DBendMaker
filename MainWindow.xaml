<Window x:Class="BendMaker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BendMaker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top">
        <Button Width="100" Height="32" Content="Import" FontWeight="Bold" FontSize="18" Click="OnImport_Click"/>
        <Button Width="100" Height="32" Content="Export" FontWeight="Bold" FontSize="18" Click="OnExport_Click"/>
    </StackPanel>
</Window>
